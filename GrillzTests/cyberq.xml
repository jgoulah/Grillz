    <CONTROL>
        <TIMEOUT_ACTION>0</TIMEOUT_ACTION>
        <COOKHOLD>4750</COOKHOLD>
        <ALARMDEV>500</ALARMDEV>
        <COOK_RAMP>0</COOK_RAMP>
        <OPENDETECT>1</OPENDETECT>
        <CYCTIME>6</CYCTIME>
        <PROPBAND>300</PROPBAND>
